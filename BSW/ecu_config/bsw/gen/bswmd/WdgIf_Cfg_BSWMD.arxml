        <!-- Start: Interfaces of WdgIf (API's) used to invoke code segment -->
        <AR-PACKAGE>
          <SHORT-NAME>BswModuleEntry</SHORT-NAME>
          <ELEMENTS>
            <BSW-MODULE-ENTRY>
              <SHORT-NAME>WdgIf_SetMode</SHORT-NAME>  <!-- Start: Std_ReturnType WdgIf_SetMode(uint8 DeviceIndex,WdgIf_ModeType WdgMode); -->
              <LONG-NAME>
                    <L-4 L="EN">Maps WdgIf_SetMode to service Wdg_SetMode of the corresponding Watchdog Driver.</L-4>
              </LONG-NAME>
              <DESC>
                    <L-2 L="EN">The Watchdog Interface module maps the service WdgIf_SetMode to the service Wdg_SetMode of the corresponding Watchdog Driver.The service WdgIf_SetMode changes the mode of the lower layer watchdog drivers which will lead to longer(Slow Mode), shorter(Fast Mode) or turn off(Off Mode) timeout periods.</L-2>
              </DESC>
              <SERVICE-ID>1</SERVICE-ID>
              <IS-REENTRANT>false</IS-REENTRANT>
              <IS-SYNCHRONOUS>true</IS-SYNCHRONOUS>
              <CALL-TYPE>REGULAR</CALL-TYPE>
              <EXECUTION-CONTEXT>UNSPECIFIED</EXECUTION-CONTEXT>
              <SW-SERVICE-IMPL-POLICY>STANDARD</SW-SERVICE-IMPL-POLICY>
              <RETURN-TYPE>
                <SHORT-NAME>ReturnValue</SHORT-NAME>
				<DESC>
					<L-2 L="EN">E_OK - Success(execution successful).E_NOT_OK - Failure (execution failed).</L-2>
				</DESC>
                <CATEGORY>TYPE_REFERENCE</CATEGORY>
                <DIRECTION>OUT</DIRECTION>
                <SW-DATA-DEF-PROPS>
                  <SW-DATA-DEF-PROPS-VARIANTS>
                    <SW-DATA-DEF-PROPS-CONDITIONAL>
                      <IMPLEMENTATION-DATA-TYPE-REF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Std/ImplementationDataTypes/Std_ReturnType</IMPLEMENTATION-DATA-TYPE-REF>
                      <SW-IMPL-POLICY>STANDARD</SW-IMPL-POLICY>
                    </SW-DATA-DEF-PROPS-CONDITIONAL>
                  </SW-DATA-DEF-PROPS-VARIANTS>
                </SW-DATA-DEF-PROPS>
              </RETURN-TYPE>
              <ARGUMENTS>
                <SW-SERVICE-ARG>
                  <SHORT-NAME>DeviceIndex</SHORT-NAME>
				  <DESC>
						<L-2 L="EN">(In)Used to identify the watchdog driver instance.</L-2>
				  </DESC>
                  <CATEGORY>TYPE_REFERENCE</CATEGORY>
                  <DIRECTION>IN</DIRECTION>
                    <SW-DATA-DEF-PROPS>
                        <SW-DATA-DEF-PROPS-VARIANTS>
                            <SW-DATA-DEF-PROPS-CONDITIONAL>
                        <IMPLEMENTATION-DATA-TYPE-REF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</IMPLEMENTATION-DATA-TYPE-REF>
                                <SW-IMPL-POLICY>STANDARD</SW-IMPL-POLICY>
                            </SW-DATA-DEF-PROPS-CONDITIONAL>
                        </SW-DATA-DEF-PROPS-VARIANTS>
                    </SW-DATA-DEF-PROPS>
                </SW-SERVICE-ARG>                                   
                <SW-SERVICE-ARG>
                  <SHORT-NAME>WdgMode</SHORT-NAME>
				  <DESC>
						<L-2 L="EN">(In)Watchdog driver mode.</L-2>
				  </DESC>
                  <CATEGORY>TYPE_REFERENCE</CATEGORY>
                  <DIRECTION>IN</DIRECTION>
                  <SW-DATA-DEF-PROPS>
                    <SW-DATA-DEF-PROPS-VARIANTS>
                      <SW-DATA-DEF-PROPS-CONDITIONAL>
                                <IMPLEMENTATION-DATA-TYPE-REF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_WdgIf/ImplementationDataTypes/WdgIf_ModeType</IMPLEMENTATION-DATA-TYPE-REF>
                        <SW-IMPL-POLICY>STANDARD</SW-IMPL-POLICY>
                      </SW-DATA-DEF-PROPS-CONDITIONAL>
                    </SW-DATA-DEF-PROPS-VARIANTS>
                  </SW-DATA-DEF-PROPS>
                </SW-SERVICE-ARG>
              </ARGUMENTS>
            </BSW-MODULE-ENTRY> <!-- End: Std_ReturnType WdgIf_SetMode(uint8 DeviceIndex,WdgIf_ModeType WdgMode); -->
            <BSW-MODULE-ENTRY>
              <SHORT-NAME>WdgIf_SetTriggerCondition</SHORT-NAME>
              <LONG-NAME>
                    <L-4 L="EN">Maps WdgIf_SetTriggerCondition to Wdg_SetTriggerCondition of the corresponding Watchdog Driver.</L-4>
              </LONG-NAME>
              <DESC>
                    <L-2 L="EN">The Watchdog Interface module maps the service WdgIf_SetTriggerCondition to the service Wdg_SetTriggerCondition of the corresponding Watchdog Driver. WdgIf_SetTriggerCondition provides timeout period information to lower layer drivers. The lower layer driver should not timeout for the period passed by the higher layer.</L-2>
              </DESC>
              <SERVICE-ID>2</SERVICE-ID>
              <IS-REENTRANT>false</IS-REENTRANT>
              <IS-SYNCHRONOUS>true</IS-SYNCHRONOUS>
              <CALL-TYPE>REGULAR</CALL-TYPE>
              <EXECUTION-CONTEXT>UNSPECIFIED</EXECUTION-CONTEXT>
              <SW-SERVICE-IMPL-POLICY>STANDARD</SW-SERVICE-IMPL-POLICY>
              <ARGUMENTS>
                <SW-SERVICE-ARG>
                  <SHORT-NAME>DeviceIndex</SHORT-NAME>
				  <DESC>
						<L-2 L="EN">(In)Used to identify the watchdog driver instance.</L-2>
				  </DESC>
                  <CATEGORY>TYPE_REFERENCE</CATEGORY>
                  <DIRECTION>IN</DIRECTION>
                  <SW-DATA-DEF-PROPS>
                    <SW-DATA-DEF-PROPS-VARIANTS>
                      <SW-DATA-DEF-PROPS-CONDITIONAL>
                        <IMPLEMENTATION-DATA-TYPE-REF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</IMPLEMENTATION-DATA-TYPE-REF>
                        <SW-IMPL-POLICY>STANDARD</SW-IMPL-POLICY>
                      </SW-DATA-DEF-PROPS-CONDITIONAL>
                    </SW-DATA-DEF-PROPS-VARIANTS>
                  </SW-DATA-DEF-PROPS>
                </SW-SERVICE-ARG>
                <SW-SERVICE-ARG>
                  <SHORT-NAME>Timeout</SHORT-NAME>
				  <DESC>
						<L-2 L="EN">(In)Timeout value for setting the trigger counter.</L-2>
				  </DESC>
                  <CATEGORY>TYPE_REFERENCE</CATEGORY>
                  <DIRECTION>IN</DIRECTION>
                  <SW-DATA-DEF-PROPS>
                    <SW-DATA-DEF-PROPS-VARIANTS>
                      <SW-DATA-DEF-PROPS-CONDITIONAL>
                        <IMPLEMENTATION-DATA-TYPE-REF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint16</IMPLEMENTATION-DATA-TYPE-REF>
                      </SW-DATA-DEF-PROPS-CONDITIONAL>
                    </SW-DATA-DEF-PROPS-VARIANTS>
                  </SW-DATA-DEF-PROPS>
                </SW-SERVICE-ARG>
              </ARGUMENTS>
            </BSW-MODULE-ENTRY>
            
            
          </ELEMENTS>
        </AR-PACKAGE>
        <!-- End: Interfaces of WdgIf (API's) used to invoke code segment -->

       <!-- Start: CompuMethod for typedef enum WdgIf_ModeType -->
        <AR-PACKAGE>
          <SHORT-NAME>CompuMethods</SHORT-NAME>
          <ELEMENTS>
            <COMPU-METHOD>
              <SHORT-NAME>WdgIf_ModeType</SHORT-NAME>
              <CATEGORY>TEXTTABLE</CATEGORY>
              <COMPU-INTERNAL-TO-PHYS>
                <COMPU-SCALES>
                  <COMPU-SCALE>
                    <DESC>
                      <L-2 L="EN">In this mode, the watchdog driver is disabled (switched off).</L-2>
                    </DESC>
                    <LOWER-LIMIT INTERVAL-TYPE="CLOSED">0</LOWER-LIMIT>
                    <UPPER-LIMIT INTERVAL-TYPE="CLOSED">0</UPPER-LIMIT>
                    <COMPU-CONST>
                      <VT>WDGIF_OFF_MODE</VT>
                    </COMPU-CONST>
                  </COMPU-SCALE>
                  <COMPU-SCALE>
                    <DESC>
                      <L-2 L="EN">In this mode, the watchdog driver is set up for a long timeout period (slow triggering).</L-2>
                    </DESC>
                    <LOWER-LIMIT INTERVAL-TYPE="CLOSED">1</LOWER-LIMIT>
                    <UPPER-LIMIT INTERVAL-TYPE="CLOSED">1</UPPER-LIMIT>
                    <COMPU-CONST>
                      <VT>WDGIF_SLOW_MODE</VT>
                    </COMPU-CONST>
                  </COMPU-SCALE>
                  <COMPU-SCALE>
                    <DESC>
                      <L-2 L="EN">In this mode, the watchdog driver is set up for a short timeout period (fast triggering).</L-2>
                    </DESC>
                    <LOWER-LIMIT INTERVAL-TYPE="CLOSED">2</LOWER-LIMIT>
                    <UPPER-LIMIT INTERVAL-TYPE="CLOSED">2</UPPER-LIMIT>
                    <COMPU-CONST>
                      <VT>WDGIF_FAST_MODE</VT>
                    </COMPU-CONST>
                  </COMPU-SCALE>
                </COMPU-SCALES>
              </COMPU-INTERNAL-TO-PHYS>
            </COMPU-METHOD>
          </ELEMENTS>
        </AR-PACKAGE>
        <!-- End: CompuMethod for typedef enum WdgIf_ModeType-->
        
        <!-- Start: CompuMethod for typedef enum WdgIf_Rb_SwtPosn_ten -->
        <AR-PACKAGE>
          <SHORT-NAME>CompuMethods</SHORT-NAME>
          <ELEMENTS>
            <COMPU-METHOD>
              <SHORT-NAME>WdgIf_Rb_SwtPosn_ten</SHORT-NAME>
              <CATEGORY>TEXTTABLE</CATEGORY>
              <COMPU-INTERNAL-TO-PHYS>
                <COMPU-SCALES>
                  <COMPU-SCALE>
                    <DESC>
                      <L-2 L="EN">In this position, all switches closed (switches MM2ERR and ERR2WDA are closed).</L-2>
                    </DESC>
                    <LOWER-LIMIT INTERVAL-TYPE="CLOSED">0</LOWER-LIMIT>
                    <UPPER-LIMIT INTERVAL-TYPE="CLOSED">0</UPPER-LIMIT>
                    <COMPU-CONST>
                      <VT>WDGIF_RB_ALLSWTCLSD_E</VT>
                    </COMPU-CONST>
                  </COMPU-SCALE>
                  <COMPU-SCALE>
                    <DESC>
                      <L-2 L="EN">In this position, MM2ERR switch open, remaining (ERR2WDA)close.</L-2>
                    </DESC>
                    <LOWER-LIMIT INTERVAL-TYPE="CLOSED">1</LOWER-LIMIT>
                    <UPPER-LIMIT INTERVAL-TYPE="CLOSED">1</UPPER-LIMIT>
                    <COMPU-CONST>
                      <VT>WDGIF_RB_MM2ERRSWTOPN_E</VT>
                    </COMPU-CONST>
                  </COMPU-SCALE>
                  <COMPU-SCALE>
                    <DESC>
                      <L-2 L="EN">In this position, ERR2WDA switch open, remaining (MM2ERR)close.</L-2>
                    </DESC>
                    <LOWER-LIMIT INTERVAL-TYPE="CLOSED">2</LOWER-LIMIT>
                    <UPPER-LIMIT INTERVAL-TYPE="CLOSED">2</UPPER-LIMIT>
                    <COMPU-CONST>
                      <VT>WDGIF_RB_ERR2WDASWTOPN_E</VT>
                    </COMPU-CONST>
                  </COMPU-SCALE>
                  <COMPU-SCALE>
                    <DESC>
                      <L-2 L="EN">In this position, All switches opened(switches MM2ERR and ERR2WDA are opened).</L-2>
                    </DESC>
                    <LOWER-LIMIT INTERVAL-TYPE="CLOSED">3</LOWER-LIMIT>
                    <UPPER-LIMIT INTERVAL-TYPE="CLOSED">3</UPPER-LIMIT>
                    <COMPU-CONST>
                      <VT>WDGIF_RB_ALLSWTOPN_E</VT>
                    </COMPU-CONST>
                  </COMPU-SCALE>
                </COMPU-SCALES>
              </COMPU-INTERNAL-TO-PHYS>
            </COMPU-METHOD>
          </ELEMENTS>
        </AR-PACKAGE>
        <!-- End: CompuMethod for typedef enum WdgIf_Rb_SwtPosn_ten-->

        <!-- Start: Implemented datatypes-->
        <AR-PACKAGE>
          <SHORT-NAME>ImplementationDataTypes</SHORT-NAME>
          <ELEMENTS>
            <IMPLEMENTATION-DATA-TYPE>
              <SHORT-NAME>WdgIf_ModeType</SHORT-NAME>
              <LONG-NAME>
                <L-4 L="EN">Mode type of the WdgIf module</L-4>
              </LONG-NAME>
              <DESC>
                <L-2 L="EN">The WdgIf_ModeType values are passed as parameters to the watchdog drivers mode switching function.</L-2>
              </DESC>             
              <CATEGORY>TYPE_REFERENCE</CATEGORY>
              <SW-DATA-DEF-PROPS>
                <SW-DATA-DEF-PROPS-VARIANTS>
                  <SW-DATA-DEF-PROPS-CONDITIONAL>
                    <COMPU-METHOD-REF DEST="COMPU-METHOD">/AUTOSAR_WdgIf/CompuMethods/WdgIf_ModeType</COMPU-METHOD-REF>
                    <IMPLEMENTATION-DATA-TYPE-REF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_Platform/ImplementationDataTypes/uint8</IMPLEMENTATION-DATA-TYPE-REF>
                  </SW-DATA-DEF-PROPS-CONDITIONAL>
                </SW-DATA-DEF-PROPS-VARIANTS>
              </SW-DATA-DEF-PROPS>
             <TYPE-EMITTER>HEADER_FILE</TYPE-EMITTER>
            </IMPLEMENTATION-DATA-TYPE> 
          </ELEMENTS>
        </AR-PACKAGE>
        <!-- End: Implemented datatypes-->
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
