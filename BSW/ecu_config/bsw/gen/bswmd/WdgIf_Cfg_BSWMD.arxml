                  <TARGET-MODULE-ID>102</TARGET-MODULE-ID>
                </BSW-MODULE-DEPENDENCY>
                <BSW-MODULE-DEPENDENCY>
                  <SHORT-NAME>Det</SHORT-NAME>
                  <TARGET-MODULE-ID>15</TARGET-MODULE-ID>
                </BSW-MODULE-DEPENDENCY>
              </BSW-MODULE-DEPENDENCYS>
