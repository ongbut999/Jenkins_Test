              <SW-VERSION>5.0.0</SW-VERSION>
              <AR-RELEASE-VERSION>4.2.2</AR-RELEASE-VERSION>
              <BEHAVIOR-REF DEST="BSW-INTERNAL-BEHAVIOR">/AUTOSAR_WdgIf/BswModuleDescriptions/WdgIf/BswInternalBehavior</BEHAVIOR-REF>
