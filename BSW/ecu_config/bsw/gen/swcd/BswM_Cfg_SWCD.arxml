<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://autosar.org/schema/r4.0"
    xsi:schemaLocation="http://autosar.org/schema/r4.0 autosar_4-2-2.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>AUTOSAR_BswM</SHORT-NAME>
      <AR-PACKAGES>
        <AR-PACKAGE>
          <SHORT-NAME>SwComponentTypes</SHORT-NAME>
          <ELEMENTS>
            <SERVICE-SW-COMPONENT-TYPE>
              <SHORT-NAME>BswM</SHORT-NAME>
              <PORTS>
                <!-- Require Ports for Immediate/Deferred SWC Mode Request Ports -->
                <!-- Require Ports for BswMSwcModeRequest SWC Mode Request Port -->
                <R-PORT-PROTOTYPE>
                  <SHORT-NAME>RP_BswMArbitration_BswMSwcModeRequest</SHORT-NAME>
                  <REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/ModeRequestInterface/ModeRequestInterface</REQUIRED-INTERFACE-TREF>
                </R-PORT-PROTOTYPE>
                <!-- Require Ports for BswM_MRP_SWC_Network SWC Mode Request Port -->
                <R-PORT-PROTOTYPE>
                  <SHORT-NAME>RP_BswMArbitration_BswM_MRP_SWC_Network</SHORT-NAME>
                  <REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/Interfaces/ETAS_SenderReceiverInterface_uint8</REQUIRED-INTERFACE-TREF>
                </R-PORT-PROTOTYPE>
                <!-- Require Ports for Immediate/Deferred SWC Mode Notification Ports -->
                <!-- Require Ports for SWC UserCallout -->
                <!-- Provide Ports for SWC Mode Switch Ports -->
                <!-- Provide Ports for BswMRteModeRequest Action -->
                <!-- Provide Ports for for BswMGenericRequests whose type is SWC -->
              </PORTS>
              <INTERNAL-BEHAVIORS>
                <SWC-INTERNAL-BEHAVIOR>
                  <SHORT-NAME>IB_BswM</SHORT-NAME>
                  <DATA-TYPE-MAPPING-REFS>
                    <!-- Data Type Mapping sets for all SWC Request Ports, SWC Mode Switch Notifications Ports and RTE Mode Switch Actions -->
                    <DATA-TYPE-MAPPING-REF DEST="DATA-TYPE-MAPPING-SET">/AUTOSAR_BswM/ModeDeclarationGroups/ECUM_STATE_Mapping</DATA-TYPE-MAPPING-REF>
                    <DATA-TYPE-MAPPING-REF DEST="DATA-TYPE-MAPPING-SET">/AppMode_Mapping/AppMode_Mapping</DATA-TYPE-MAPPING-REF>
                    <DATA-TYPE-MAPPING-REF DEST="DATA-TYPE-MAPPING-SET">/AUTOSAR_ComM/DataTypeMappingSets/ComMModeMapping</DATA-TYPE-MAPPING-REF>
                    <DATA-TYPE-MAPPING-REF DEST="DATA-TYPE-MAPPING-SET">/AUTOSAR_Dcm/DataTypeMappingSets/DTS_DcmEcuReset</DATA-TYPE-MAPPING-REF>
                    <DATA-TYPE-MAPPING-REF DEST="DATA-TYPE-MAPPING-SET">/AUTOSAR_Dcm/DataTypeMappingSets/DTS_DcmDiagnosticSessionControl</DATA-TYPE-MAPPING-REF>
                  </DATA-TYPE-MAPPING-REFS>
                  <EVENTS>
                    <!-- Data Received event for all immediate SWC requests -->
                    <!-- Data Received event for all Deferred SWC requests -->
                    <!-- Data Received Event for BswMSwcModeRequest Deferred SWC Request Ports -->
                    <DATA-RECEIVED-EVENT>
                      <SHORT-NAME>DRE_BswM_Cfg_DfrdSwcReqst_BswMSwcModeRequest</SHORT-NAME>
                      <START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/AUTOSAR_BswM/SwComponentTypes/BswM/IB_BswM/RE_BswM_Cfg_DfrdSwcReqst_BswMSwcModeRequest</START-ON-EVENT-REF>
                      <DATA-IREF>
                        <CONTEXT-R-PORT-REF DEST="R-PORT-PROTOTYPE">/AUTOSAR_BswM/SwComponentTypes/BswM/RP_BswMArbitration_BswMSwcModeRequest</CONTEXT-R-PORT-REF>
                        <TARGET-DATA-ELEMENT-REF DEST="VARIABLE-DATA-PROTOTYPE">/ModeRequestInterface/ModeRequestInterface/AppMode</TARGET-DATA-ELEMENT-REF>
                      </DATA-IREF>
                    </DATA-RECEIVED-EVENT>
                    <!-- Data Received Event for BswM_MRP_SWC_Network Deferred SWC Request Ports -->
                    <DATA-RECEIVED-EVENT>
                      <SHORT-NAME>DRE_BswM_Cfg_DfrdSwcReqst_BswM_MRP_SWC_Network</SHORT-NAME>
                      <START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">/AUTOSAR_BswM/SwComponentTypes/BswM/IB_BswM/RE_BswM_Cfg_DfrdSwcReqst_BswM_MRP_SWC_Network</START-ON-EVENT-REF>
                      <DATA-IREF>
                        <CONTEXT-R-PORT-REF DEST="R-PORT-PROTOTYPE">/AUTOSAR_BswM/SwComponentTypes/BswM/RP_BswMArbitration_BswM_MRP_SWC_Network</CONTEXT-R-PORT-REF>
                        <TARGET-DATA-ELEMENT-REF DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/ETAS_SenderReceiverInterface_uint8/uint8</TARGET-DATA-ELEMENT-REF>
                      </DATA-IREF>
                    </DATA-RECEIVED-EVENT>
                    <!-- Mode Switch events for all immediate SWC Notifications -->
                    <!-- Mode Switch events for all Deferred SWC Notifications -->
                    <!-- Operation Invoked Event for BswMGenericRequests whose type is SWC -->
                  </EVENTS>
                  <RUNNABLES>
                    <!-- Runnable Entities for all Immediate SWC Request Ports -->
                    <!-- Runnable Entities for all Deferred SWC Request Ports -->
                    <!-- Runnable Entity for BswMSwcModeRequest Deferred SWC Mode Request Port -->
                    <RUNNABLE-ENTITY>
                      <SHORT-NAME>RE_BswM_Cfg_DfrdSwcReqst_BswMSwcModeRequest</SHORT-NAME>
                      <CAN-BE-INVOKED-CONCURRENTLY>true</CAN-BE-INVOKED-CONCURRENTLY>
                      <DATA-RECEIVE-POINT-BY-ARGUMENTS>
                        <VARIABLE-ACCESS>
                          <SHORT-NAME>DRP_BswMSwcModeRequest</SHORT-NAME>
                          <ACCESSED-VARIABLE>
                            <AUTOSAR-VARIABLE-IREF>
                              <PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/AUTOSAR_BswM/SwComponentTypes/BswM/RP_BswMArbitration_BswMSwcModeRequest</PORT-PROTOTYPE-REF>
                              <TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/ModeRequestInterface/ModeRequestInterface/AppMode</TARGET-DATA-PROTOTYPE-REF>
                            </AUTOSAR-VARIABLE-IREF>
                          </ACCESSED-VARIABLE>
                        </VARIABLE-ACCESS>
                      </DATA-RECEIVE-POINT-BY-ARGUMENTS>
                      <SYMBOL>BswM_Cfg_DfrdSwcReqst_BswMSwcModeRequest</SYMBOL>
                    </RUNNABLE-ENTITY>
                    <!-- Runnable Entity for BswM_MRP_SWC_Network Deferred SWC Mode Request Port -->
                    <RUNNABLE-ENTITY>
                      <SHORT-NAME>RE_BswM_Cfg_DfrdSwcReqst_BswM_MRP_SWC_Network</SHORT-NAME>
                      <CAN-BE-INVOKED-CONCURRENTLY>true</CAN-BE-INVOKED-CONCURRENTLY>
                      <DATA-RECEIVE-POINT-BY-ARGUMENTS>
                        <VARIABLE-ACCESS>
                          <SHORT-NAME>DRP_BswM_MRP_SWC_Network</SHORT-NAME>
                          <ACCESSED-VARIABLE>
                            <AUTOSAR-VARIABLE-IREF>
                              <PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/AUTOSAR_BswM/SwComponentTypes/BswM/RP_BswMArbitration_BswM_MRP_SWC_Network</PORT-PROTOTYPE-REF>
                              <TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/Interfaces/ETAS_SenderReceiverInterface_uint8/uint8</TARGET-DATA-PROTOTYPE-REF>
                            </AUTOSAR-VARIABLE-IREF>
                          </ACCESSED-VARIABLE>
                        </VARIABLE-ACCESS>
                      </DATA-RECEIVE-POINT-BY-ARGUMENTS>
                      <SYMBOL>BswM_Cfg_DfrdSwcReqst_BswM_MRP_SWC_Network</SYMBOL>
                    </RUNNABLE-ENTITY>
                    <!-- Runnable Entities for all Immediate SWC Notification Ports -->
                    <!-- Runnable Entities for all Deferred SWC Notification Ports -->
                    <!-- Runnable Entities for Rte_Switch actions -->
                    <!-- Runnable Entities for BswMRteModeRequest actions -->
                    <!-- Runnable Entity for BswMGenericRequests whose type is SWC -->
                  </RUNNABLES>
                  <SUPPORTS-MULTIPLE-INSTANTIATION>false</SUPPORTS-MULTIPLE-INSTANTIATION>
                </SWC-INTERNAL-BEHAVIOR>
              </INTERNAL-BEHAVIORS>
            </SERVICE-SW-COMPONENT-TYPE>
          </ELEMENTS>
        </AR-PACKAGE>
        <AR-PACKAGE>
          <SHORT-NAME>SwcImplementations</SHORT-NAME>
          <ELEMENTS>
            <SWC-IMPLEMENTATION>
              <SHORT-NAME>SWCIMPL_BswM</SHORT-NAME>
              <CODE-DESCRIPTORS>
                <CODE>
                  <SHORT-NAME>CodeDescriptor</SHORT-NAME>
                  <ARTIFACT-DESCRIPTORS>
                    <AUTOSAR-ENGINEERING-OBJECT>
                      <SHORT-LABEL>ArEngObj</SHORT-LABEL>
                      <CATEGORY>SWSRC</CATEGORY>
                    </AUTOSAR-ENGINEERING-OBJECT>
                  </ARTIFACT-DESCRIPTORS>
                </CODE>
              </CODE-DESCRIPTORS>
              <PROGRAMMING-LANGUAGE>C</PROGRAMMING-LANGUAGE>
              <SW-VERSION>1.0.0</SW-VERSION>
              <BEHAVIOR-REF DEST="SWC-INTERNAL-BEHAVIOR">/AUTOSAR_BswM/SwComponentTypes/BswM/IB_BswM</BEHAVIOR-REF>
            </SWC-IMPLEMENTATION>
          </ELEMENTS>
        </AR-PACKAGE>
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
